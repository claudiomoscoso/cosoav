<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name>cosoav</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <context-param>
    <param-name>cosoav.web.refresh</param-name>
    <param-value>60</param-value>
  </context-param>
  <context-param>
    <param-name>cosoav.database.driver</param-name>
    <param-value>org.gjt.mm.mysql.Driver</param-value>
  </context-param>
  <context-param>
    <param-name>cosoav.database.server</param-name>
    <param-value>localhost</param-value>
  </context-param>
  <context-param>
    <param-name>cosoav.database.database</param-name>
    <param-value>cosoav</param-value>
  </context-param>
  <context-param>
    <param-name>cosoav.database.username</param-name>
    <param-value>root</param-value>
  </context-param>
  <context-param>
    <param-name>cosoav.database.password</param-name>
    <param-value>admin</param-value>
  </context-param>
</web-app>